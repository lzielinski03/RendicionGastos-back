        </dependency>

        <dependency>
            <groupId>io.jsonwebtoken</groupId>
            <artifactId>jjwt</artifactId>
            <version>0.6.0</version>
        </dependency>
    </dependencies>

    <properties>
        <start-class>ar.com.besysoft.Application</start-class>
        <java.version>1.8</java.version>
